{
  "name": "simple-proxy",
  "version": "3.0.3",
  "engineStrict": true,
  "description": "简易反向代理，支持 WEB 界面管理，支持 HTTPS",
  "main": "server/app.js",
  "scripts": {
    "dev": "nodemon -e js,yaml server/app.js",
    "logs": "nodemon -e js,yaml server/logs.js",
    "lint": "eslint --ext .js server",
    "test": "NODE_ENV=test mocha --require co-mocha --colors server/lib/test/mocha_bootload 'server/**/*.test.js'",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter html --reporter text-summary mocha --require co-mocha --colors server/lib/test/mocha_bootload 'server/**/*.test.js'",
    "start": "pm2 start pm2.json",
    "postmerge": "git submodule update && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/qious/simple-proxy.git"
  },
  "keywords": [
    "proxy"
  ],
  "author": "qiujun",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "chance": "^1.0.4",
    "co-mocha": "^1.1.3",
    "eslint": "^3.1.1",
    "mocha": "^3.0.0",
    "nyc": "^7.1.0",
    "supertest-session": "^2.0.1"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.0",
    "co": "^4.6.0",
    "co-request": "^1.0.0",
    "config": "^1.21.0",
    "connect-redis": "^3.1.0",
    "express": "^4.13.4",
    "express-ko": "^1.0.1",
    "express-session": "^1.14.0",
    "glob": "^7.1.1",
    "http-proxy": "^1.15.1",
    "husky": "^0.13.3",
    "influx": "^5.0.0-alpha.4",
    "ioredis": "^2.3.0",
    "lodash": "^4.15.0",
    "moder": "^1.3.2",
    "mysql": "^2.11.1",
    "nodemon": "^1.10.2",
    "request-stats": "^1.0.1",
    "sequelize": "^3.24.1",
    "ua-parser-js": "^0.7.12",
    "wechat-enterprise-api": "git+https://github.com/qious/wechat-enterprise-api.git",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.2.0"
  }
}
